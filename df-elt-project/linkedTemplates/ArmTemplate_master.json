{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "df-elt-project"
		},
		"ls_databricks_ws_accessToken": {
			"type": "secureString",
			"metadata": "Secure string for 'accessToken' of 'ls_databricks_ws'"
		},
		"ls_datalake_gold_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ls_datalake_gold'"
		},
		"ls_datalake_raw_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ls_datalake_raw'"
		},
		"ls_db_logs_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'ls_db_logs'"
		},
		"ls_synapse_dpool_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'ls_synapse_dpool'"
		},
		"ls_databricks_ws_properties_typeProperties_existingClusterId": {
			"type": "string",
			"defaultValue": "0730-234038-ylufwtl2"
		},
		"ls_datalake_gold_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://datalakeetlproject.dfs.core.windows.net/"
		},
		"ls_datalake_raw_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://datalakeetlproject.dfs.core.windows.net/"
		},
		"ls_db_logs_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "sv-eltproject.database.windows.net"
		},
		"ls_db_logs_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "db_logs_pipelines"
		},
		"ls_db_logs_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "gscamba"
		},
		"ls_fmp_api_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://financialmodelingprep.com/api/v3/"
		},
		"ls_keyvault_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://kvcambaapikeys.vault.azure.net/"
		},
		"ls_synapse_dpool_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "sy-elt-project.sql.azuresynapse.net"
		},
		"ls_synapse_dpool_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "dpool_eltproject"
		},
		"ls_synapse_dpool_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "sqladminuser"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "df-elt-project_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ls_databricks_ws_accessToken": {
						"value": "[parameters('ls_databricks_ws_accessToken')]"
					},
					"ls_datalake_gold_accountKey": {
						"value": "[parameters('ls_datalake_gold_accountKey')]"
					},
					"ls_datalake_raw_accountKey": {
						"value": "[parameters('ls_datalake_raw_accountKey')]"
					},
					"ls_db_logs_password": {
						"value": "[parameters('ls_db_logs_password')]"
					},
					"ls_synapse_dpool_password": {
						"value": "[parameters('ls_synapse_dpool_password')]"
					},
					"ls_databricks_ws_properties_typeProperties_existingClusterId": {
						"value": "[parameters('ls_databricks_ws_properties_typeProperties_existingClusterId')]"
					},
					"ls_datalake_gold_properties_typeProperties_url": {
						"value": "[parameters('ls_datalake_gold_properties_typeProperties_url')]"
					},
					"ls_datalake_raw_properties_typeProperties_url": {
						"value": "[parameters('ls_datalake_raw_properties_typeProperties_url')]"
					},
					"ls_db_logs_properties_typeProperties_server": {
						"value": "[parameters('ls_db_logs_properties_typeProperties_server')]"
					},
					"ls_db_logs_properties_typeProperties_database": {
						"value": "[parameters('ls_db_logs_properties_typeProperties_database')]"
					},
					"ls_db_logs_properties_typeProperties_userName": {
						"value": "[parameters('ls_db_logs_properties_typeProperties_userName')]"
					},
					"ls_fmp_api_properties_typeProperties_url": {
						"value": "[parameters('ls_fmp_api_properties_typeProperties_url')]"
					},
					"ls_keyvault_properties_typeProperties_baseUrl": {
						"value": "[parameters('ls_keyvault_properties_typeProperties_baseUrl')]"
					},
					"ls_synapse_dpool_properties_typeProperties_server": {
						"value": "[parameters('ls_synapse_dpool_properties_typeProperties_server')]"
					},
					"ls_synapse_dpool_properties_typeProperties_database": {
						"value": "[parameters('ls_synapse_dpool_properties_typeProperties_database')]"
					},
					"ls_synapse_dpool_properties_typeProperties_userName": {
						"value": "[parameters('ls_synapse_dpool_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}